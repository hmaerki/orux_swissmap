{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Lint",
      "type": "python",
      "request": "launch",
      "module": "pylint",
      "cwd": "${workspaceFolder}",
      "args": [
        "--rcfile=${workspaceFolder}/.pylintrc",
        "--verbose",
        "*.py",
        "programm"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Black",
      "type": "python",
      "request": "launch",
      "module": "black",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config",
        "${workspaceFolder}/.blackconfig.yaml",
        "*.py",
        "programm/*.py"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: ZuercherOberland.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/ZuercherOberland.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: TestGeoTIFF.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/TestGeoTIFF.py",
      "console": "integratedTerminal"
    }
  ]
}